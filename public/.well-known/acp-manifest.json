{
    "schema_version": "1.0",
    "protocol": "acp",
    "merchant": {
        "name": "JSONMart",
        "legal_entity": "JSONMart B2B Marketplace",
        "domain": "beanhull0357-bot.github.io",
        "url": "https://beanhull0357-bot.github.io/ai-market/",
        "country": "KR",
        "currency": "KRW",
        "support_email": "support@jsonmart.io",
        "merchant_category": "B2B Office Supplies & Consumables"
    },
    "capabilities": {
        "instant_checkout": true,
        "product_feed": {
            "url": "https://beanhull0357-bot.github.io/ai-market/feeds/acp-products.json",
            "rpc_endpoint": "get_acp_feed",
            "format": "acp_product_feed",
            "update_frequency": "realtime",
            "item_count": 50,
            "categories": [
                "CONSUMABLES",
                "MRO"
            ]
        },
        "checkout": {
            "protocol": "UCP",
            "session_endpoint": "ucp_create_session",
            "complete_endpoint": "ucp_complete_session",
            "payment_model": "authorize_capture",
            "auth_hold_hours": 24,
            "supported_actions": [
                "authorize",
                "capture",
                "void"
            ]
        },
        "shipping": {
            "countries": [
                "KR"
            ],
            "free_shipping": true,
            "min_handling_days": 1,
            "max_handling_days": 2,
            "estimated_delivery_days": {
                "min": 2,
                "max": 5
            }
        },
        "returns": {
            "policy_url": "https://beanhull0357-bot.github.io/ai-market/#/policies/returns",
            "default_return_days": 7,
            "free_returns": false,
            "return_fee_range": {
                "min": 0,
                "max": 10000,
                "currency": "KRW"
            }
        }
    },
    "api": {
        "type": "supabase_rpc",
        "base_url": "https://{project_ref}.supabase.co/rest/v1/rpc/",
        "authentication": {
            "required": false,
            "note": "Product feed and checkout are public. Order management requires API key."
        },
        "endpoints": {
            "product_feed": "get_acp_feed",
            "create_checkout": "ucp_create_session",
            "complete_checkout": "ucp_complete_session",
            "agent_register": "agent_self_register",
            "product_catalog": "get_product_feed"
        },
        "documentation": "https://beanhull0357-bot.github.io/ai-market/#/agent/docs",
        "openapi_spec": "https://beanhull0357-bot.github.io/ai-market/openapi.json"
    },
    "trust": {
        "agent_reviews": true,
        "review_verdicts": [
            "ENDORSE",
            "WARN",
            "BLOCKLIST"
        ],
        "trust_score_range": [
            0,
            100
        ],
        "average_trust_score": 85,
        "total_products": 50,
        "total_reviews": 18
    },
    "compliance": {
        "data_handling": "All data processed via Supabase (SOC2 certified). No PII stored without consent.",
        "payment_security": "Authorize/Capture model with 24h hold. No direct card handling.",
        "dispute_resolution": "Manual admin review for all orders."
    }
}